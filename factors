#!/usr/bin/env python3
import sys

def find_factors(n):
    """Find two factors of a number n."""
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return i, n // i
    return n, 1  # If no factors found, return the number itself and 1

def process_file(filename):
    """Process each number in the file and print its factorization."""
    try:
        with open(filename, 'r') as file:
            for line in file:
                n = int(line.strip())
                if n > 1:
                    p, q = find_factors(n)
                    print(f"{n}={p}*{q}")
    except FileNotFoundError:
        print(f"Error: File '{filename}' not found")
        sys.exit(1)
    except ValueError:
        print("Error: Invalid number in file")
        sys.exit(1)
    except Exception as e:
        print(f"Error: {e}")
        sys.exit(1)

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)
    
    filename = sys.argv[1]
    process_file(filename)

if __name__ == "__main__":
    main()
